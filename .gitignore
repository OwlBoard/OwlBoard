# OwlBoard .gitignore

# Environment variables
.env
.env.local
.env.*.local

# SSL/TLS Certificates and Keys
Secure_Channel/ca/*.key
Secure_Channel/ca/*.srl
Secure_Channel/certs/*/*.key
Secure_Channel/certs/*/*.csr
*.pem
*.p12
*.pfx

# Docker
.docker/
docker-compose.override.yml

# Logs
*.log
logs/
*.log.*

# OS Files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
*~

# IDE
.vscode/
.idea/
*.swp
*.swo
*.swn
.project
.classpath
.settings/
*.sublime-*

# Node
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.npm
.eslintcache

# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg
MANIFEST
.pytest_cache/
.coverage
htmlcov/
.tox/
.venv
venv/
ENV/
env/

# Go
*.exe
*.exe~
*.dll
*.so
*.dylib
*.test
*.out
vendor/

# Dart/Flutter
.dart_tool/
.flutter-plugins
.flutter-plugins-dependencies
.packages
.pub-cache/
.pub/
build/
flutter_*.log

# Database dumps
*.sql
*.dump
*.sqlite
*.db

# Backup files
*.bak
*.backup
*.old
*.orig

# Temporary files
tmp/
temp/
*.tmp

# Documentation build
docs/_build/
site/

# Keep certificate directories but ignore the actual certificates
!Secure_Channel/ca/.gitkeep
!Secure_Channel/certs/*/.gitkeep

# Keep public certificates (CA certificate is needed)
!Secure_Channel/ca/ca.crt
!Secure_Channel/certs/*/server.crt
!Secure_Channel/certs/*/client.crt

# Test coverage
coverage/
.nyc_output/

# Secrets and credentials
secrets/
credentials/
*.credentials
*secret*
*token*
*password*

# Except these documentation files
!*SECRET*.md
!*PASSWORD*.md
